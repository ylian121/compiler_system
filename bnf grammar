prog:
  | fn prog

fn: 
Func Ident LeftParen param_list RightParen Return block

param_list:
  | Int Ident Comma param_list
  | Int Ident

Return: 
  | Ident

block: 
LeftCurly stmts RightCurly

stmts: 
  | stmt stmts

stmt: 
Ident Assign expr
  | LeftParen Num RightParen Ident Assign expr Semicolon
  | Ident Ident Semicolon
  | Int Ident Semicolon
  | Ident Ident LeftParen Num RightParen Semicolon
  | Break Semicolon
  | Continue Semicolon
  | While expr block
  | If expr block Else expr
  | If expr block

expr:
Num
  | Ident
  | Ident LeftParen Num RightParen
  | LeftParen Plus expr expr RightParen
  | LeftParen Subtract expr expr RightParen
  | LeftParen Ident expr_list RightParen

expr_list:
  | expr expr_list

