prog:
  | fn prog

func: 
Func Ident LeftParen param_list RightParen block

param_list:
  | Int Ident Comma param_list
  | Int Ident

Return: 
  | Ident

block: 
LeftCurly stmts RightCurly

stmts: 
  | stmt stmts

stmt: 
Ident Assign expr
  | LeftParen Num RightParen Ident Assign expr 
  | Ident Ident Semicolon
  | Int Ident Semicolon
  | Ident Ident LeftParen Num RightParen 
  | Break Semicolon
  | Continue Semicolon
  | While expr block
  | If expr block Else expr
  | If expr block
  | Print LeftParen Num RightParen Semicolon
  | Print LeftParen Ident RightParen Semicolon

expr:
Num
  | Num expr
  | Num Semicolon
  | Ident Semicolon
  | Ident LeftParen Num RightParen Semicolon
  | LeftParen Plus expr expr RightParen Semicolon
  | LeftParen Subtract expr expr RightParen Semicolon
  | LeftParen Ident expr_list RightParen Semicolon
  | Plus expr expr Semicolon
  | Subtract expr expr Semicolon
  | Ident expr_list Semicolon
  | Ident Less expr
  | Ident LessEqual expr
  | Ident Greater expr
  | Ident GreaterEqual expr
  | Ident Equality expr
  | Ident NotEqual expr


expr_list:
  | expr expr_list

